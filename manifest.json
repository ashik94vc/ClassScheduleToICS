{
  "manifest_version":   2,
  "name":               "SBU Class Schedule Exporter",
  "version":            "1.2",
  "description":        "Export SBU class schedule as ICS which can be imported into Google Calendar",
  "icons": { "128": "icon.png" },
  "content_scripts":    [
    {
      "matches":          ["https://psns.cc.stonybrook.edu/*"],
      "all_frames":       true,
      "js":               ["jquery-1.9.1.min.js", "content_script.js"]
    }
  ]
}
